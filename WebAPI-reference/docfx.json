{
  "metadata": [
    {
      "src": [
        {
          "files": ["actions/**", "complextypes/**", "entitytypes/**", "enumtypes/**", "functions/**", "metadataentitytypes/**", "queryfunctions/**", "solutions/**"]
        }
      ],
      "dest": "./"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
		  "remarks/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {},
    "fileMetadata": {},
    "template": [],
    "dest": "WebAPI-reference"
  }
}
